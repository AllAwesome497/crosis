{
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "./dist",
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "strict": true,
    "lib": ["es7", "dom", "esnext.asynciterable"],
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "declaration": true,
    "noImplicitAny": true,
    "removeComments": true
  },
  "include": ["src", "protocol"]
}
