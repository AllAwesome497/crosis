{
  "name": "@replit/crosis",
  "version": "1.0.0",
  "description": "Goval connection and channel manager",
  "main": "index.js",
  "scripts": {
    "gen": "pbjs -t static-module -w commonjs -o api.js api.proto && pbts -o api.d.ts api.js",
    "lint": "eslint . --ext .js,.ts",
    "build:standalone": "webpack --config ./webpack.config.js",
    "docs": "typedoc --out docs --tsconfig ../../tsconfig.json --includeDeclarations --exclude \"**/!(api).d.ts\" --excludePrivate --mode modules client.ts channel.ts api.d.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "faris@repl.it",
  "license": "UNLICENSED",
  "dependencies": {
    "engine.io-client": "^3.4.0",
    "protobufjs": "^6.8.8"
  },
  "devDependencies": {
    "@types/engine.io-client": "^3.1.2",
    "@types/node": "^12.7.5",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "prettier": "^1.18.2",
    "ts-loader": "^5.3.3",
    "typedoc": "^0.14.2",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  }
}
